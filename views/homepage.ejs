      <div class="row">
        <%- include side-nav.ejs%>
        <%- include graph.ejs%>
        <div class="progress" id="loadingDiv">
          <div class="progress-bar progress-bar-info progress-bar-striped" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%">
            <span class="sr-only">40% Complete</span>
          </div>
        </div>
        <div class="col-md-10" id="page-port">
          <%- include modal-add-action.ejs%>
          <%- include table-port.ejs%>
          <%- include collapse.ejs%>
        </div>
        <div class="col-md-10" id="page-stock">
          <%- include table-stock.ejs%>
        </div>
      </div>
    </div>
  </div>
</div>
